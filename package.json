{
  "name": "elysium-webext",
  "version": "0.1.0",
  "private": true,
  "prettier": {
    "tabWidth": 4
  },
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf build && next build && next export -o build",
    "export": "next export -o build",
    "start": "next start",
    "webext": "web-ext run --verbose --source-dir ./build --firefox=/Applications/Firefox\\ Developer\\ Edition.app/Contents/MacOS/firefox-bin --firefox-profile=elysium  --keep-profile-changes"
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@reduxjs/toolkit": "^1.5.0",
    "next": "^10.0.6",
    "polished": "^4.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@zeit/next-workers": "^1.0.0",
    "typescript": "^4.1.3",
    "web-ext": "^5.5.0",
    "webextension-polyfill-ts": "^0.24.0",
    "worker-loader": "^2.0.0"
  },
  "//resolutions": {
    "webpack": "^5.21.2"
  }
}
